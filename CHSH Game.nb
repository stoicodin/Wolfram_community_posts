        RowBox[{
         RowBox[{"\"\<RY\>\"", "[", "\[Phi]0", "]"}], "->", "4"}], ",", 
        RowBox[{"{", "3", "}"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"\"\<C\>\"", ",", 
        RowBox[{
         RowBox[{"\"\<RY\>\"", "[", "\[Phi]1", "]"}], "->", "4"}], ",", 
        RowBox[{"{", "3", "}"}]}], "}"}], ",", 
      RowBox[{"{", "1", "}"}], ",", 
      RowBox[{"{", "2", "}"}], ",", 
      RowBox[{"{", "3", "}"}], ",", 
