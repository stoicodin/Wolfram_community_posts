          RowBox[{"MatrixLog", "[", 
           RowBox[{"mn1", "[", "\"\<DensityMatrix\>\"", "]"}], "]"}], "-", 
          RowBox[{"MatrixLog", "[", 
           RowBox[{"mn2", "[", "\"\<DensityMatrix\>\"", "]"}], "]"}]}], 
         ")"}]}], "]"}], "]"}], "/", 
     RowBox[{"Log", "[", "2", "]"}]}], ",", "\"\<Bits\>\""}], 
